Table: v3Char
Define: Generate=[|Average|AboveAvg|Superhuman|God]
Define: Average=10
Define: AboveAvg=20
Define: Superhuman=40
Define: God=100
Set: MIND={1d{${$Generate}}}
Set: SENSES={1d{${$Generate}}}
Set: BODY={1d{${$Generate}}}
Set: ARMOR1={1d{${$Generate}}}
New {$Generate} Character<hr>MIND={$MIND}<br>SENSES={$SENSES}<br>BODY={$BODY}<br><br>HP={$BODY*5}<br>ARMOR={$ARMOR1}<hr>